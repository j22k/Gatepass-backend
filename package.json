  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "node -r dotenv/config -e \"const { execSync } = require('child_process'); execSync('npx node-pg-migrate up', { stdio: 'inherit' });\"",
    "migrate:down": "node -r dotenv/config -e \"const { execSync } = require('child_process'); execSync('npx node-pg-migrate down', { stdio: 'inherit' });\"",
    "migrate:create": "npx node-pg-migrate create",
    "seed": "node -r dotenv/config seeds/run-seeds.js",
    "start": "nodemon src/server.js",
    "dev": "nodemon src/server.js"
  },
